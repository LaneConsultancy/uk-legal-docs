{
  "rewrites": [
    { "source": "/gumroad", "destination": "/gumroad.html" },
    { "source": "/((?!api|gumroad\\.html|.*\\..*).*)", "destination": "/index.html" }
  ],
  "redirects": [
    {
      "source": "/:path(.*)",
      "has": [{ "type": "host", "value": "www.getlegaldocs.co.uk" }],
      "destination": "https://getlegaldocs.co.uk/:path",
      "permanent": true
    }
  ]
}
